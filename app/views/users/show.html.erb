<%= render partial: "mypage", locals: { user: @user } %>

<section class="latest-box">
  <div class="container ">
    <div class="row mb-5 align-items-center">
      <div class="col">
        <h4 class="mb-0"><%= @user.nickname %>さんの投稿一覧</h4>
        <div>Have a better date for you</div>
      </div>
    </div>
    <div class="row row-cols-3">
      <%= render partial: "posts/post", collection: @posts %>
    </div>
  </div>
</section>
